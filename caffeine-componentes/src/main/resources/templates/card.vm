<div class="card" style="width: 18rem;">
  <img src="data:image/png;base64, $card.getImagem()" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">$card.getTitulo()</h5>
    <p class="card-text">$card.getTexto()</p>
	#foreach($comp in $card.getComponentes())
		$comp.gerarSaida()
	#end
  </div>
</div>